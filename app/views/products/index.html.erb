<p id="notice"><%= notice %></p>

<h1>Products</h1>

<table>
  <thead>
    <tr>
      <th>Product name</th>
      <th>Product url</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <% @h.each do |hash| %>
     <span id="link"><%= "http://eattreat.in/products/#{hash["slug"]}" %></span>
     <span id="image_link" style="color:red;margin-left:10px;"><%=  "http://eattreat.in#{hash["master"]["images"][0]["small_url"]}" %></span>
     <br/>
     
  <% end %>
  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.product_name %></td>
        <td><%= product.product_url %></td>
        <td><%= link_to 'Show', product %></td>
        <td><%= link_to 'Edit', edit_product_path(product) %></td>
        <td><%= link_to 'Destroy', product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product', new_product_path %>
